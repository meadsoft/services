{
    "name": "@haru-cafe/cms",
    "version": "0.0.0",
    "tags": [
        "query",
        "command",
        "api",
        "haru-cafe-cms"
    ],
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "db:generate": "drizzle-kit generate --config ./src/drizzle.config.ts",
        "db:migrate": "drizzle-kit migrate --config ./src/drizzle.config.ts",
        "bundle": "tsup --config ../../tsup.config.ts"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "@haru-cafe/monorepo": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "nx": {
        "name": "cms"
    },
    "dependencies": {
        "@haru-cafe/cms-contracts": "workspace:*",
        "@haru-cafe/common": "workspace:*",
        "@haru-cafe/common-infrastructure": "workspace:*",
        "@nestjs/common": "^11.0.1",
        "@nestjs/swagger": "^11.2.3",
        "@nestjs/testing": "^11.0.1",
        "@types/pg": "^8.15.6",
        "drizzle-kit": "^0.31.7",
        "drizzle-orm": "^0.44.7",
        "nestjs-zod": "^5.0.1",
        "pg": "^8.16.3",
        "tslib": "^2.3.0",
        "tsup": "^8.5.1",
        "zod": "^4.1.12"
    }
}
